{
  "presets": [
    "es2015",
    "react",
    "stage-2",
    [ "env", {
      "targets": {
        "browsers": "> 2%"
      }
    }]
  ],

  "plugins": [
    "react-hot-loader/babel",
    "transform-react-jsx-self",
    "transform-react-jsx-source",
    "styled-components",
    "transform-decorators-legacy",
    "syntax-dynamic-import",
    "transform-class-properties"
  ]
}